{
    "name": "geoviz",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "generate-client": "openapi --input ./swagger-spec.json --output ./client --client axios",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "app:dev:build:start": "supabase start && docker compose up geoviz-dev -d --build ",
        "app:dev:start": "supabase start && docker compose up geoviz-dev -d ",
        "app:dev:stop": "supabase stop && docker compose down ",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest --config ./test/jest-e2e.json",
        "test:watch": "dotenv -v NODE_ENV=test -- jest --watch --config ./test/jest-e2e.json",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "pretest:e2e": "supabase stop && supabase start",
        "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
        "posttest:e2e": "supabase stop"
    },
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.3.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/mapped-types": "^1.2.2",
        "@nestjs/passport": "^9.0.1",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/swagger": "^6.2.1",
        "@supabase/supabase-js": "^2.7.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "helmet": "^6.0.1",
        "nestjs-supabase-auth": "^1.0.9",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.2.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "29.2.4",
        "@types/multer": "^1.4.7",
        "@types/node": "18.11.18",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "form-data-lite": "^1.0.3",
        "jest": "29.3.1",
        "openapi-typescript-codegen": "^0.23.0",
        "pactum": "^3.3.2",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supabase": "^1.36.2",
        "supertest": "^6.1.3",
        "ts-jest": "29.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.1.1",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}